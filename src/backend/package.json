  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules ./app.ts",
    "build": "tsc",
    "test": "jest",
    "db_crete": "sequelize-cli db:create",
    "db_migrate": "sequelize-cli db:migrate",
    "db_install": "sequelize-cli db:create && sequelize-cli db:migrate",
    "db_remove": "sequelize-cli db:drop",
    "db_reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "mysql2": "^2.3.3",
    "path": "^0.12.7",
    "restify": "^8.6.1",
    "restify-cors-middleware2": "^2.1.2",
    "restify-router": "^0.6.2",
    "sequelize": "^6.16.1",
    "sequelize-cli": "^6.4.1",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bluebird": "^3.5.36",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@types/restify": "^8.5.4",
    "@types/sequelize": "^4.28.11",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "jest": "^27.5.1",
    "supertest": "^6.2.2",
    "ts-node-dev": "^1.1.8"
